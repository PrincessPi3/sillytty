source $HOME/.bashrc

echo "$HOME/.bashrc"
cat $HOME/.bashrc
echo $?

echo "printenv"
printenv

echo $?

echo "SILLYDEV: $SILLYDEV"
touch $SILLYDEV
echo $?

ls -lah $SILLYDEV
echo $?

echo "SILLYTTY: $SILLYTTY"

echo "RCFILE: $HOME/.bashrc"
cat $HOME/.bashrc
echo $?

whereis start-sillyfilly
echo $?

whereis stop-sillyfilly
echo $?

whereis vardump-sillyfilly
echo $?

whereis uninstall-sillyfilly
echo $?

echo "SILLYHOST: $SILLYHOST"
echo "SILLYPORT: $SILLYPORT"
echo "SILLYPROTO: $SILLYPROTO"
echo "SILLYBAUD: $SILLYBAUD"
echo "SILLYPACKAGES: $SILLYPACKAGES"
echo "PATH: $PATH"

