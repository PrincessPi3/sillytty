source $HOME/.bashrc

echo "$HOME/.bashrc"
cat $HOME/.bashrc
echo $?

echo "printenv"
printenv

echo $?

echo "SILLYDEV: $SILLYDEV"
touch $SILLYDEV
echo $?

ls -lah $SILLYDEV
echo $?

echo "SILLYTTY: $SILLYTTY"

echo "RCFILE: $HOME/.bashrc"
cat $HOME/.bashrc
echo $?

whereis START-SILLYTTY
echo $?

whereis STOP-SILLYTTY
echo $?

whereis VARDUMP-SILLYTTY
echo $?

whereis UNINSTALL-SILLYTTY
echo $?

echo "SILLYHOST: $SILLYHOST"
echo "SILLYPORT: $SILLYPORT"
echo "SILLYPROTO: $SILLYPROTO"
echo "SILLYBAUD: $SILLYBAUD"
echo "SILLYPIPE: $SILLYPIPE"
echo "SILLYPTY: $SILLYPTY"
echo "SILLYTTY: $SILLYTTY"
# echo "SILLYPACKAGES: $SILLYPACKAGES"
echo "PATH: $PATH"